    <test name="Login wrong password iPhone 11 Pro">
        <parameter name="device-name" value="iphone.11pro" />
        <parameter name="ios-version" value="13.3" />
        <classes>
            <class name="testcases.login.iOS.LoginWrongPassiOS"/>
